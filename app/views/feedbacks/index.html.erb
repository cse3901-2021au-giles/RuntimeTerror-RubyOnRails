<p id="notice"><%= notice %></p>

<h1>Feedbacks</h1>

<table>
  <thead>
    <tr>
      <th>Fid</th>
      <th>Team</th>
      <th>Course</th>
      <th>Giveuser</th>
      <th>Receiveuser</th>
      <th>Done</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr>
        <td><%= feedback.fid %></td>
        <td><%= feedback.team_id %></td>
        <td><%= feedback.course_id %></td>
        <td><%= feedback.giveuser %></td>
        <td><%= feedback.receiveuser %></td>
        <td><%= feedback.done %></td>
        <td><%= feedback.body %></td>
        <td><%= link_to 'Show', feedback %></td>
        <td><%= link_to 'Edit', edit_feedback_path(feedback) %></td>
        <td><%= link_to 'Destroy', feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feedback', new_feedback_path %>
