<% content_for :title, "Completed Feedback" %>
<div class="center"> 

<table class="table">
  <thead>
    <tr>
      <th scope="col">Due</th>
      <th scope="col">Project Name</th>
      <th scope="col">Team Name</th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
    <%@assigned_checkpoints.each do |checkpoint| %>
        <tr>
            <td><%=checkpoint.due_date%></td>
            <td><%=checkpoint.checkpoint_name%></td>
            <td><%=@teams.find(checkpoint.id).team_name%></td>
            <td><%=link_to 'View', feedback_view_completed_path(checkpoint.id)%></td>
        </tr>
    <% end %>
    
  </tbody>
</table>

</div>
