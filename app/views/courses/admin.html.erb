<!-- Lets layout know what the title is-->
<% content_for :title, "User Courses"%>
<p> <%= Current.user.fname%></p>

<table>
  <tr>
    <th> Course Name </th>
    <th> Actions </th>
  </tr>
  <% Current.user.courses.each do |c|%>
    <tr>
      <td><%= c.course_name %></td>
      <td> Remove Students </td>
      <td> Delete </td>
    </tr>
  <%end%>
  <tr>
    <td> Add Class</td>
  </tr>
</table>
